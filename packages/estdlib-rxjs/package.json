{
  "name": "@marcj/estdlib-rxjs",
  "version": "0.1.19",
  "description": "engineering stdlib for typescript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/",
  "author": "Marc J. Schmidt <marc@marcjschmidt.de>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "jest --coverage",
    "tsc": "rm -rf dist && tsc",
    "tsc-watch": "rm -rf dist && tsc --watch"
  },
  "peerDependencies": {
    "@marcj/estdlib": "^0.1.11",
    "rxjs": "^6.3.3"
  },
  "devDependencies": {
    "@marcj/estdlib": "^0.1.19",
    "rxjs": "^6.3.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "transformIgnorePatterns": [],
    "testMatch": [
      "**/tests/**/*.+(ts|tsx|js)"
    ],
    "setupTestFrameworkScriptFile": "jest-extended"
  },
  "globals": {
    "ts-jest": {
      "tsConfigFile": "tsconfig.json"
    }
  }
}
